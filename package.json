  },
  "homepage": "https://github.com/d6u/River",
  "devDependencies": {
    "babel": "^5.8.23",
    "babel-core": "^5.8.25",
    "babel-eslint": "^4.1.3",
    "babel-loader": "^5.3.2",
    "eslint": "^1.5.0",
    "eslint-config-airbnb": "0.0.8",
    "eslint-plugin-react": "^3.4.2",
